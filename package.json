{
  "name": "vuetify3-admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./node_modules/.v",
    "dev": "vite --mode dev",
    "prod": "vite --mode prod",
    "build_local": "yarn gv_dev && vite build --mode local",
    "build_dev": "yarn gv_dev && vite build --mode dev",
    "build_prod": "yarn gv_prod && vite build --mode prod",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "preview": "vite preview",
    "gv_local": "cross-env NODE_ENV=local node generate-build-version ",
    "gv_dev": "cross-env NODE_ENV=dev node generate-build-version ",
    "gv_prod": "cross-env NODE_ENV=prod node generate-build-version ",
    "lint": "eslint ./ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint-fix": "eslint ./ --ext .vue,.js,.cjs,.mjs,.jsx --fix --color --cache --max-warnings=-1",
    "format": "prettier --write ./ --ext .vue,.js,.jsx,.cjs,.mjs,.yml,.json,.html",
    "prepare": "husky install",
    "pre": "npm run  format && npm run lint-fix",
    "g": "node .\\generators\\cmd.mjs make"
  },
  "dependencies": {
    "@chenfengyuan/vue-countdown": "^2.1.1",
    "@iconify/vue": "^4.1.1",
    "@mdi/font": "^7.2.96",
    "@toast-ui/editor": "^3.2.2",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.1.0",
    "@toast-ui/editor-plugin-color-syntax": "^3.1.0",
    "@vee-validate/i18n": "^4.9.3",
    "@vuepic/vue-datepicker": "^5.1.0",
    "@vueuse/core": "^10.1.2",
    "apexcharts": "^3.40.0",
    "axios": "^1.4.0",
    "colord": "^2.9.3",
    "core-js": "^3.30.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "filepond": "^4.30.4",
    "filepond-plugin-file-poster": "^2.5.1",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "filepond-plugin-image-preview": "^4.6.11",
    "filepond-plugin-image-validate-size": "^1.2.7",
    "flag-icons": "^6.6.6",
    "lodash-es": "^4.17.21",
    "maska": "^2.1.9",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.36",
    "print-js": "^1.6.0",
    "prismjs": "^1.29.0",
    "qs": "^6.11.1",
    "roboto-fontface": "*",
    "tippy": "^0.0.0",
    "tui-editor-plugin-font-size": "^1.0.4",
    "ua-parser-js": "^1.0.35",
    "vee-validate": "^4.9.3",
    "vue": "^3.3.2",
    "vue-filepond": "^7.0.3",
    "vue-fullscreen": "^3.1.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0",
    "vue-simple-acl": "^2.0.2",
    "vue-spinner": "^1.0.4",
    "vue-timer-hook": "^1.0.80",
    "vue-toastification": "^2.0.0-rc.5",
    "vue2-dropzone-vue3": "^1.1.0",
    "vue3-apexcharts": "^1.4.1",
    "vue3-easy-data-table": "^1.5.42",
    "vue3-otp-input": "^0.4.1",
    "vue3-perfect-scrollbar": "^1.6.1",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.2.4",
    "webfontloader": "^1.6.28",
    "xgplayer": "^3.0.2",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "axios-mock-adapter": "^1.21.4",
    "babel-eslint": "^10.1.0",
    "chalk": "5.2.0",
    "clear": "^0.1.0",
    "commander": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.12.0",
    "execa": "^7.1.1",
    "husky": "^8.0.3",
    "jszip": "^3.10.1",
    "lint-staged": "^13.2.2",
    "mkdirp": "^3.0.1",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "ssh2": "^1.13.0",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.5",
    "unplugin-vue-macros": "^2.1.6",
    "vite": "^4.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vite-plugin-zip-pack": "^1.0.5",
    "yargs": "^17.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
