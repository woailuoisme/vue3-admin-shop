{
  "name": "vuetify3-admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve_dev": "vite --mode dev",
    "serve_prod": "vite --mode prod",
    "build_local": "yarn gv_dev && vite build --mode local",
    "build_dev": "yarn gv_dev && vite build --mode dev",
    "build_prod": "yarn gv_prod && vite build --mode prod",
    "preview": "vite preview",
    "gv_local": "cross-env NODE_ENV=local node generate-build-version ",
    "gv_dev": "cross-env NODE_ENV=dev node generate-build-version ",
    "gv_prod": "cross-env NODE_ENV=prod node generate-build-version ",
    "lint": "eslint ./ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint-fix": "eslint ./ --ext .vue,.js,.jsx --fix --color --cache --max-warnings=-1",
    "format": "prettier --write ./ --ext .vue,.js,.jsx ",
    "prepare": "husky install",
    "pre": "yarn format && yarn lint-fix",
    "g": "node .\\generators\\cmd.js make"
  },
  "dependencies": {
    "@chenfengyuan/vue-countdown": "^2.1.0",
    "@iconify/vue": "^4.1.0",
    "@mdi/font": "^7.1.96",
    "@vuepic/vue-datepicker": "^4.0.1",
    "@vueuse/core": "^9.13.0",
    "apexcharts": "^3.37.0",
    "axios": "^1.3.2",
    "colord": "^2.9.3",
    "core-js": "^3.8.3",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "filepond": "^4.30.4",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "filepond-plugin-image-preview": "^4.6.11",
    "flag-icons": "^6.6.6",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "pinia-plugin-persistedstate": "^3.0.2",
    "print-js": "^1.6.0",
    "qs": "^6.11.1",
    "roboto-fontface": "*",
    "seemly": "^0.3.6",
    "swiper": "^9.1.0",
    "ua-parser-js": "^1.0.34",
    "unplugin-vue-define-options": "^1.2.4",
    "vditor": "^3.9.1",
    "vee-validate": "^4.7.4",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.2.13",
    "vue-filepond": "^7.0.3",
    "vue-fullscreen": "^3.1.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0",
    "vue-simple-acl": "^2.0.2",
    "vue-toastification": "^2.0.0-rc.5",
    "vue2-dropzone-vue3": "^1.1.0",
    "vue3-apexcharts": "^1.4.1",
    "vue3-easy-data-table": "^1.5.31",
    "vue3-otp-input": "^0.3.6",
    "vue3-perfect-scrollbar": "^1.6.1",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.1.6",
    "wangeditor": "^4.7.15",
    "webfontloader": "^1.0.0",
    "xgplayer": "^2.32.2",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.9.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "axios-mock-adapter": "^1.21.2",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "mkdirp": "^2.1.5",
    "mockjs": "^1.1.0",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.59.3",
    "tailwindcss": "^3.2.7",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "yargs": "^17.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
